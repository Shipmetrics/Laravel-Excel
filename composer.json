{
  "name": "maatwebsitevthree/excelv3",
  "description": "Supercharged Excel exports in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "php",
    "phpspreadsheet",
    "phpexcel",
    "excel",
    "csv",
    "export",
    "import",
    "batch"
  ],
  "authors": [
    {
      "name": "Patrick Brouwers",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": "^7.0",
    "phpoffice/phpspreadsheet": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1",
    "orchestra/testbench": "^3.6",
    "orchestra/database": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Maatwebsitevthree\\Excel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maatwebsitevthree\\Excel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Maatwebsitevthree\\Excel\\ExcelServiceProvider"
      ],
      "aliases": {
        "Excelvthree": "Maatwebsitevthree\\Excel\\Facades\\Excelvthree"
      }
    }
  }
}
